 <url-pattern>/T003_EditServlet</url-pattern>
</servlet-mapping>

<servlet>
 <servlet-name>T002_DeleteServlet</servlet-name>
 <servlet-class>fjs.cs.action.T002_DeleteServlet</servlet-class>
</servlet>
 
